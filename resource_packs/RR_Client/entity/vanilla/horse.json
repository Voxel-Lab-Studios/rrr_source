        "particle.c": "controller.animation.lab.horse_particle_effect.c"
      },
      "scripts": {
        "initialize": [
          "v.eat_anim = 0.0;"
        ],
        "pre_animation": [
          "v.armor_texture_slot = q.armor_texture_slot(1);",
          "v.gear_texture_slot = q.variant;",
          "v.skin_texture_slot = q.skin_id;",
          "v.eat_anim = math.clamp(v.eat_anim + (q.is_grazing ? ((1.0 - v.eat_anim) * 0.4 + 0.05) : -v.eat_anim * 0.4 - 0.05) * q.delta_time * 20.0, 0.0, 1.0);",
          "v.head_x_rot = q.target_x_rotation + (q.modified_move_speed > 0.2 ? (math.cos(q.modified_distance_moved * 11.46) * 11.46 * q.modified_move_speed + q.modified_move_speed * 11.46) : 0.0);",
          "v.inverse_max_stand_eat = (1.0 - math.max(v.stand_anim, v.eat_anim));",
          "v.leg_stand_factor = math.cos((q.modified_distance_moved * 38.38) + 180.0);",
          "v.leg_x_rot_anim = v.leg_stand_factor * 45.8 * q.modified_move_speed;"
        ],
        "animate": [
          { "baby_transform": "q.is_baby" },
          "walk",
          "look_at_player",
          "particle.c",
          { "rear": "v.stand_anim > 0.0" },
          { "eat": "!q.is_standing" },
          { "tail": "v.shake_tail" },
          { "sit": "q.is_sitting && !q.is_moving" }
        ]
      },
      "render_controllers": [
        {"controller.render.lab.horse": "q.skin_id != 44 && q.skin_id != 45"},
        {"controller.render.lab.unicorn": "q.skin_id == 44"},
        {"controller.render.lab.void": "q.skin_id == 45"}

